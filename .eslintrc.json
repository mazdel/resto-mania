{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jasmine": true,
    "codeceptjs/codeceptjs": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "plugins": ["codeceptjs"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}
